   "source": [
    "## 5. Carte thermique de l'affluence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "source": [
    "# Préparation des données pour la carte thermique\n",
    "if 'day_of_week_num' not in df_processed.columns:\n",
    "    df_processed['day_of_week_num'] = df_processed.index.dayofweek\n",
    "\n",
    "if 'hour' not in df_processed.columns and df_processed.index.inferred_freq != 'D':\n",
    "    df_processed['hour'] = df_processed.index.hour\n",
    "    \n",
    "    # Carte thermique hebdomadaire-horaire\n",
    "    pivot_df = df_processed.pivot_table(\n",
    "        values='patient_count',\n",
    "        index='hour',\n",
    "        columns='day_of_week_num',\n",
    "        aggfunc='mean'\n",
    "    )\n",
    "    \n",
    "    # Renommer les colonnes pour les jours de la semaine\n",
    "    pivot_df.columns = ['Lundi', 'Mardi', 'Mercredi', 'Jeudi', 'Vendredi', 'Samedi', 'Dimanche']\n",
    "    \n",
    "    # Visualisation avec Plotly\n",
    "    fig = px.imshow(\n",
    "        pivot_df,\n",
    "        labels=dict(x=\"Jour de la semaine\", y=\"Heure\", color=\"Nombre de patients\"),\n",
    "        x=pivot_df.columns,\n",
    "        y=pivot_df.index,\n",
    "        color_continuous_scale=\"YlOrRd\"\n",
    "    )\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title='Carte thermique de l\\'affluence par jour de la semaine et heure',\n",
    "        height=600\n",
    "    )\n",
    "    \n",
    "    fig.show()\n",
    "else:\n",
    "    # Carte thermique mensuelle-hebdomadaire si données journalières\n",
    "    # Ajout des colonnes nécessaires\n",
    "    df_processed['week_of_year'] = df_processed.index.isocalendar().week\n",
    "    \n",
    "    pivot_df = df_processed.pivot_table(\n",
    "        values='patient_count',\n",
    "        index='month',\n",
    "        columns='day_of_week_num',\n",
    "        aggfunc='mean'\n",
    "    )\n",
    "    \n",
    "    # Renommer les colonnes pour les jours de la semaine\n",
    "    pivot_df.columns = ['Lundi', 'Mardi', 'Mercredi', 'Jeudi', 'Vendredi', 'Samedi', 'Dimanche']\n",
    "    \n",
    "    # Renommer les indices pour les mois\n",
    "    pivot_df.index = [calendar.month_name[m] for m in pivot_df.index]\n",
    "    \n",
    "    # Visualisation avec Plotly\n",
    "    fig = px.imshow(\n",
    "        pivot_df,\n",
    "        labels=dict(x=\"Jour de la semaine\", y=\"Mois\", color=\"Nombre moyen de patients\"),\n",
    "        x=pivot_df.columns,\n",
    "        y=pivot_df.index,\n",
    "        color_continuous_scale=\"YlOrRd\"\n",
    "    )\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title='Carte thermique de l\\'affluence par mois et jour de la semaine',\n",
    "        height=600\n",
    "    )\n",
    "    \n",
    "    fig.show()"
   ]
  },
