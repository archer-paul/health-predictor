   "source": [
    "## 6. Visualisation des prédictions sur une année"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "source": [
    "# Chargement du modèle ARIMA pour les prédictions\n",
    "try:\n",
    "    arima_model_path = '../models/arima_model.pkl'\n",
    "    with open(arima_model_path, 'rb') as f:\n",
    "        arima_model = pickle.load(f)\n",
    "    print(\"Modèle ARIMA chargé avec succès.\")\n",
    "    \n",
    "    # Génération de prédictions pour une année (365 jours)\n",
    "    days_to_predict = 365\n",
    "    last_date = df_processed.index[-1]\n",
    "    future_dates = pd.date_range(start=last_date + pd.Timedelta(days=1), periods=days_to_predict)\n",
    "    \n",
    "    # Prédictions avec intervalle de confiance\n",
    "    forecast = arima_model.predict(steps=days_to_predict, return_conf_int=True)\n",
    "    forecast_mean = forecast[0]\n",
    "    forecast_ci = forecast[1]\n",
    "    \n",
    "    # Création d'un DataFrame pour les prédictions\n",
    "    forecast_df = pd.DataFrame({\n",
    "        'prediction': forecast_mean,\n",
    "        'lower_ci': forecast_ci[:, 0],\n",
    "        'upper_ci': forecast_ci[:, 1]\n",
    "    }, index=future_dates)\n",
    "    \n",
    "    # Visualisation des prédictions avec Plotly\n",
    "    fig = go.Figure()\n",
    "    \n",
    "    # Données historiques\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=df_processed.index,\n",
    "        y=df_processed['patient_count'],\n",
    "        mode='lines',\n",
    "        name='Données historiques',\n",
    "        line=dict(color='royalblue')\n",
    "    ))\n",
    "    \n",
    "    # Prédictions\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=forecast_df.index,\n",
    "        y=forecast_df['prediction'],\n",
    "        mode='lines',\n",
    "        name='Prédictions',\n",
    "        line=dict(color='red')\n",
    "    ))\n",
    "    \n",
    "    # Intervalle de confiance\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=forecast_df.index.tolist() + forecast_df.index.tolist()[::-1],\n",
    "        y=forecast_df['upper_ci'].tolist() + forecast_df['lower_ci'].tolist()[::-1],\n",
    "        fill='toself',\n",
    "        fillcolor='rgba(255,0,0,0.2)',\n",
    "        line=dict(color='rgba(255,0,0,0)'),\n",
    "        hoverinfo='skip',\n",
    "        showlegend=True,\n",
    "        name='Intervalle de confiance 95%'\n",
    "    ))\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title='Prédictions d\\'affluence hospitalière pour l\\'année à venir',\n",
    "        xaxis_title='Date',\n",
    "        yaxis_title='Nombre de patients',\n",
    "        hovermode='x unified',\n",
    "        height=600,\n",
    "        legend=dict(orientation='h', yanchor='bottom', y=1.02, xanchor='right', x=1)\n",
    "    )\n",
    "    \n",
    "    fig.update_layout(\n",
    "        xaxis=dict(\n",
    "            rangeslider=dict(visible=True),\n",
    "            type='date'\n",
    "        )\n",
    "    )\n",
    "    \n",
    "    fig.show()\n",
    "    \n",
    "except Exception as e:\n",
    "    print(f\"Erreur lors du chargement du modèle ARIMA: {e}\")\n",
    "    print(\"Passez à l'étape suivante ou entraînez d'abord le modèle ARIMA.\")"
   ]
  },
