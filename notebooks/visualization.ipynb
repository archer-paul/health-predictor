   "metadata": {},
   "source": [
    "# Chargement des deux modèles\n",
    "try:\n",
    "    # Chargement du modèle LSTM\n",
    "    lstm_model_path = '../models/lstm_model.pkl'\n",
    "    with open(lstm_model_path, 'rb') as f:\n",
    "        lstm_model = pickle.load(f)\n",
